@model IEnumerable<IExpro.Core.Models.Reports.PurchaseRawMaterialModel>

<div class="row">
    <div class="col-lg-12">
        <div class="table-responsive">
            <table class="table table-hover table-bordered table-striped">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Category)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.PONo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.POStatus)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.PODate)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.SupplierName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ItemName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Rate)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.POQty)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.DelvDate)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.DelayDays)
                    </th>


                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Category)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PONo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.POStatus)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PODate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SupplierName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ItemName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Rate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.POQty)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DelvDate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DelayDays)
                        </td>

                    </tr>
                }

            </table>
        </div>
    </div>
</div>